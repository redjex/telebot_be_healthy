import telebot
from telebot import types
import time


token = '–¢–æ–∫–µ–Ω'
bot = telebot.TeleBot(token)


@bot.message_handler(commands=['start'])
def start(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    item1 = types.KeyboardButton("–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –≤—Ä–∞—á–∞ üë©‚Äç‚öïÔ∏è")
    item2 = types.KeyboardButton("–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –±–æ—Ç–∞ ü§ñ")
    markup.add(item1, item2)
    bot.send_message(message.chat.id, "–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ, {0.first_name}! –Ø —á–∞—Ç-–±–æ—Ç, –∫–æ—Ç–æ—Ä—ã–π –ø–æ–º–æ–∂–µ—Ç –≤–∞–º –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –≤–∞—à–∏–º–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞–º–∏ –∞–Ω–∞–ª–∏–∑–∞ –∫—Ä–æ–≤–∏ –∏ –ø–æ–ª—É—á–∏—Ç—å —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ –¥–ª—è –Ω–æ—Ä–º–∞–ª–∏–∑–∞—Ü–∏–∏ —Å–≤–æ–µ–≥–æ –∑–¥–æ—Ä–æ–≤—å—è.".format(message.from_user))
    bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ –¥–æ—Å—Ç—É–ø–Ω—ã–π –≤–∞—Ä–∏–∞–Ω—Ç –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏–∏:", reply_markup=markup)

@bot.message_handler(commands = ['help'])
def help(message):
  markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
  back = types.KeyboardButton('–ù–∞–∑–∞–¥‚¨ÖÔ∏è')
  markup.add(back)
  markup1 = types.ReplyKeyboardMarkup(resize_keyboard=True)
  item1 = types.KeyboardButton("–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –≤—Ä–∞—á–∞ üë©‚Äç‚öïÔ∏è")
  item2 = types.KeyboardButton("–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –±–æ—Ç–∞ ü§ñ")
  markup.add(item1, item2)
  bot.send_message(message.chat.id, '–í—Å–µ –∫–æ–º–∞–Ω–¥—ã, –∫–æ—Ç–æ—Ä—ã–µ –º–æ–≥—É—Ç –≤–∞–º –ø—Ä–∏–≥–æ–¥–∏—Ç—å—Å—è: \n /help, \n /start', reply_markup=markup)



@bot.message_handler(content_types=['text'])
def message_reply(message):
    print(message.text)
    if message.text == "–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –≤—Ä–∞—á–∞ üë©‚Äç‚öïÔ∏è":
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        bot.send_message(message.chat.id, "https://www.krasn-gb1.ru/")
        back = types.KeyboardButton('–ù–∞–∑–∞–¥‚¨ÖÔ∏è')
        markup.add(back)
        bot.send_message(message.chat.id, "–í–æ—Ç —Å—Å—ã–ª–∫–∞ –Ω–∞ —Å–∞–π—Ç \n–¢–∞–º –≤—ã —Å–º–æ–∂–µ—Ç–µ –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ –ø—Ä–∏–µ–º \n–•–æ—Ä–æ—à–µ–≥–æ –¥–Ω—è.üëç", reply_markup=markup)


    elif message.text == "–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –±–æ—Ç–∞ ü§ñ":
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        item1 = types.KeyboardButton("–≠—Ä–∏—Ç—Ä–æ—Ü–∏—Ç—ã")
        item2 = types.KeyboardButton("–õ–µ–π–∫–æ—Ü–∏—Ç—ã")
        item3 = types.KeyboardButton("–ì–µ–º–æ–≥–ª–æ–±–∏–Ω")
        item4 = types.KeyboardButton("–¢—Ä–æ–º–±–æ—Ü–∏—Ç—ã")
        back =  types.KeyboardButton("–ù–∞–∑–∞–¥‚¨ÖÔ∏è")
        markup.add(item1, item2)
        markup.add(item3, item4)
        markup.add(back)
        bot.send_message(message.chat.id, "–û—Ç–º–µ—Ç—å—Ç–µ –ø–∞—Ä–∞–º–µ—Ç—Ä –∫—Ä–æ–≤–∏ –¥–ª—è –≤—ã–¥–∞—á–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –∏ –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏–∏.", reply_markup=markup)

    elif message.text == "–≠—Ä–∏—Ç—Ä–æ—Ü–∏—Ç—ã":
        bot.send_message(message.chat.id, "–û—Ç–ø—Ä–∞–≤—å—Ç–µ –í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å:")
        bot.register_next_step_handler(message, er_check)
    elif message.text == "–õ–µ–π–∫–æ—Ü–∏—Ç—ã":
        bot.send_message(message.chat.id, "–û—Ç–ø—Ä–∞–≤—å—Ç–µ –í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å:")
        bot.register_next_step_handler(message, lk_check)
    elif message.text == "–ì–µ–º–æ–≥–ª–æ–±–∏–Ω":
        bot.send_message(message.chat.id, "–û—Ç–ø—Ä–∞–≤—å—Ç–µ –í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å:")
        bot.register_next_step_handler(message, gm_check)
    elif message.text == "–¢—Ä–æ–º–±–æ—Ü–∏—Ç—ã":
        bot.send_message(message.chat.id, "–û—Ç–ø—Ä–∞–≤—å—Ç–µ –í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å:")
        bot.register_next_step_handler(message, tb_check)
    elif message.text == "–ù–∞–∑–∞–¥‚¨ÖÔ∏è":
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        item1 = types.KeyboardButton("–û–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ –≤—Ä–∞—á—Éüë©‚Äç‚öïÔ∏è")
        item2 = types.KeyboardButton("–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –±–æ—Ç–∞ü§ñ")
        markup.add(item1, item2)
        bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ –≤–∞—Ä–∏–∞–Ω—Ç –≤–æ–ø—Ä–æ—Å–∞:", reply_markup=markup)


def er_check(message):
    if float(message.text) > 5.5:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–≤—ã—à–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n–í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å —ç—Ä–µ—Ç—Ä–æ—Ü–∏—Ç–æ–≤ –∑–∞–≤—ã—à–µ–Ω.  –ß—Ç–æ –±—ã –ø–æ–≤—ã—Å–∏—Ç—å –∏—Ö, –≤—ã –¥–æ–ª–∂–Ω—ã: –†–µ–≥—É–ª—è—Ä–Ω–æ –∑–∞–Ω–∏–º–∞—Ç—å—Å—è —Ñ–∏–∑–∏—á–µ—Å–∫–æ–π –∞–∫—Ç–∏–≤–Ω–æ—Å—Ç—å—é, —Ç–∞–∫–æ–π –∫–∞–∫  —Ö–æ–¥—å–±–∞, –ø–ª–∞–≤–∞–Ω—å–µ –∏–ª–∏ –π–æ–≥–∞; –ò–∑–±–µ–≥–∞–π—Ç–µ –¥–ª–∏—Ç–µ–ª—å–Ω–æ–≥–æ —Å–∏–¥–µ–Ω–∏—è –∏–ª–∏ —Å—Ç–æ—è–Ω–∏—è –≤ –æ–¥–Ω–æ–π –ø–æ–∑–µ; –°–æ–∫—Ä–∞—Ç–∏—Ç–µ –ø–æ—Ç—Ä–µ–±–ª–µ–Ω–∏–µ –ø—Ä–æ–¥—É–∫—Ç–æ–≤, –±–æ–≥–∞—Ç—ã—Ö –∂–µ–ª–µ–∑–æ–º, —Ç–∞–∫–∏—Ö –∫–∞–∫ –∫—Ä–∞—Å–Ω–æ–µ –º—è—Å–æ –∏ –ø–µ—á–µ–Ω—å.")
    elif float(message.text) < 4.5:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–Ω–∏–∂–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n–í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å —ç—Ä–∏—Ç—Ä–æ—Ü–∏—Ç–æ–≤ –Ω–∏–∂–µ –Ω–æ—Ä–º—ã. –ü–æ–≤—ã—Å–∏—Ç—å –∏—Ö –≤–∞–º –ø–æ–º–æ–∂–µ—Ç —Å–±–∞–ª–∞–Ω—Å–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ –ø–∏—Ç–∞–Ω–∏–µ —Å –∞–∫—Ü–µ–Ω—Ç–æ–º –Ω–∞ –≤–∞–∂–Ω—ã–µ –ø–∏—Ç–∞—Ç–µ–ª—å–Ω—ã–µ –≤–µ—â–µ—Å—Ç–≤–∞ ‚Äì –ª—É—á—à–∏–π —Å–ø–æ—Å–æ–± –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞—Ç—å –∑–¥–æ—Ä–æ–≤—ã–µ —ç—Ä–∏—Ç—Ä–æ—Ü–∏—Ç—ã –∏ –≥–µ–º–æ–≥–ª–æ–±–∏–Ω. –í —Ä–∞—Ü–∏–æ–Ω–µ –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –∫—Ä–∞—Å–Ω–æ–µ –º—è—Å–æ, –ø–µ—á–µ–Ω—å, —Ä—ã–±–∞, –ª–∏—Å—Ç–æ–≤—ã–µ –æ–≤–æ—â–∏ (–∫–∞–ø—É—Å—Ç–∞, —à–ø–∏–Ω–∞—Ç), —á–µ—á–µ–≤–∏—Ü–∞, —Ñ–∞—Å–æ–ª—å, –≥–æ—Ä–æ—Ö, –æ—Ä–µ—Ö–∏ –∏ —Å—É—à–µ–Ω—ã–µ —è–≥–æ–¥—ã. –¢–∞–∫–∂–µ –Ω—É–∂–Ω–æ –æ–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ –≤—Ä–∞—á—É.")
    else:
        bot.send_message(message.chat.id, "–í—Å–µ –≤ –Ω–æ—Ä–º–µüòç")
        bot.send_message(message.chat.id, "–°–æ–≤–µ—Ç–∞ –Ω–µ –±—É–¥–µ—ÇüòÅ")
def lk_check(message):
    if float(message.text) > 9000:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–≤—ã—à–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n–í–∞—à–∏ –ª–µ–π–∫–æ—Ü–∏—Ç—ã –≤—ã—à–µ –Ω–æ—Ä–º—ã. \n–í–∞—à —Ä–∞—Ü–∏–æ–Ω –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å —Å–±–∞–ª–∞–Ω—Å–∏—Ä–æ–≤–∞–Ω –∏ –≤–∫–ª—é—á–∞—Ç—å –±–µ–ª–∫–æ–≤—É—é –ø–∏—â—É, —Ä—ã–±—É, –∑–ª–∞–∫–∏, —Ñ—Ä—É–∫—Ç—ã –∏ –æ–≤–æ—â–∏. \n–õ–µ–≥–∫–∏–π –∫—É—Ä–∏–Ω—ã–π –±—É–ª—å–æ–Ω ‚Äì –∏–¥–µ–∞–ª—å–Ω–∞—è –ø–∏—â–∞ –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è —á–∏—Å–ª–∞ –ª–µ–π–∫–æ—Ü–∏—Ç–æ–≤. –ü—Ä–∏ –∏—Ö —Å–Ω–∏–∂–µ–Ω–∏–∏ —Ä–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è —É–ø–æ—Ç—Ä–µ–±–ª—è—Ç—å —Ç–∞–∫–∂–µ –∫–∏—Å–ª–æ–º–æ–ª–æ—á–Ω—ã–µ –ø—Ä–æ–¥—É–∫—Ç—ã –∏ –∫—Ä–∞—Å–Ω—ã–µ —Å–æ—Ä—Ç–∞ —Ä—ã–±. –í —Ä–∞—Ü–∏–æ–Ω–µ –∂–µ–ª–∞—Ç–µ–ª—å–Ω–æ –∏–º–µ—Ç—å –∫—Ä–µ–≤–µ—Ç–∫–∏, –º–æ—Ä—Å–∫—É—é.")
    elif float(message.text) < 4000:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–Ω–∏–∂–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n –í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å –ª–µ–π–∫–æ—Ü–∏—Ç–æ–≤ –Ω–∏–∂–µ –Ω–æ—Ä–º—ã. \n–í–∞–º —Å–ª–µ–¥—É–µ—Ç –Ω–µ–º–µ–¥–ª–µ–Ω–Ω–æ –Ω—É–∂–Ω–æ –æ–±—Ä–∞—Ç–∏—Ç—å—Å—è –≤ –±–æ–ª—å–Ω–∏—Ü—É –ø—Ä–∏ –ø–æ—è–≤–ª–µ–Ω–∏–∏ —Å–∏–Ω—è–∫–æ–≤ –Ω–∞ —Ç–µ–ª–µ, –Ω–µ–æ–±–æ—Å–Ω–æ–≤–∞–Ω–Ω–æ–≥–æ –ø–æ–≤—ã—à–µ–Ω–∏—è —Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä—ã —Ç–µ–ª–∞, —á—Ä–µ–∑–º–µ—Ä–Ω–æ–π —Å–ª–∞–±–æ—Å—Ç–∏ –∏ —É—Å—Ç–∞–ª–æ—Å—Ç–∏. \n–ö–∞–∫ –ø—Ä–∞–≤–∏–ª–æ, –ø–æ–Ω–∏–∂–µ–Ω–Ω—ã–µ –ª–µ–π–∫–æ—Ü–∏—Ç—ã –≤ –∫—Ä–æ–≤–∏ ‚Äì —Å–ª–µ–¥—Å—Ç–≤–∏–µ –∏–Ω—Ñ–µ–∫—Ü–∏–æ–Ω–Ω–æ–≥–æ –∑–∞–±–æ–ª–µ–≤–∞–Ω–∏—è, –ø–æ—ç—Ç–æ–º—É –ø–∞—Ü–∏–µ–Ω—Ç—É –Ω–∞–∑–Ω–∞—á–∞—é—Ç –∞–Ω—Ç–∏–±–∏–æ—Ç–∏–∫–∏ —à–∏—Ä–æ–∫–æ–≥–æ —Å–ø–µ–∫—Ç—Ä–∞ –¥–µ–π—Å—Ç–≤–∏—è.")
    else:
        bot.send_message(message.chat.id, "–í—Å–µ –≤ –Ω–æ—Ä–º–µüòç")
        bot.send_message(message.chat.id, "–°–æ–≤–µ—Ç–∞ –Ω–µ –±—É–¥–µ—ÇüòÅ")
def gm_check(message):
    if float(message.text) > 160:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–≤—ã—à–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n–í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å –≥–µ–º–æ–≥–ª–æ–±–∏–Ω–∞ –≤—ã—à–µ –Ω–æ—Ä–º—ã. –í–∞–º –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –æ–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ –≤—Ä–∞—á—É. –¢–∞–∫–∂–µ –ø–æ–Ω–∏–∑–∏—Ç—å –≥–µ–º–æ–≥–ª–æ–±–∏–Ω –ø–æ–º–æ–∂–µ—Ç –ø—Ä–∞–≤–∏–ª—å–Ω–æ–µ –ø–∏—Ç–∞–Ω–∏–µ. –ú–æ–∂–Ω–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å —Ç–∞–∫–∏–µ –ø—Ä–æ–¥—É–∫—Ç—ã:\n–†–∏—Å–æ–≤–∞—è –∫—Ä—É–ø–∞.\n–§—Ä—É–∫—Ç—ã –∂–µ–ª—Ç–æ–≥–æ –∏ –∑–µ–ª–µ–Ω–æ–≥–æ —Ü–≤–µ—Ç–∞ (—Å–ª–∏–≤—ã, –≥—Ä—É—à–∏, –±–µ–ª–∞—è —Å–º–æ—Ä–æ–¥–∏–Ω–∞, k–∏—Å–ª–æ–º–æ–ª–æ—á–Ω—ã–µ –ø—Ä–æ–¥—É–∫—Ç—ã).\n–†—ã–±–∞ –Ω–µ–∂–∏—Ä–Ω—ã—Ö —Å–æ—Ä—Ç–æ–≤, –∂–µ–ª–∞—Ç–µ–ª—å–Ω–æ —Å –º—è—Å–æ–º –±–µ–ª–æ–≥–æ —Ü–≤–µ—Ç–∞.\n–ú–∞–∫–∞—Ä–æ–Ω–Ω—ã–µ –∏–∑–¥–µ–ª–∏—è –∏–∑ —Ü–µ–ª—å–Ω–æ–∑–µ—Ä–Ω–æ–≤–æ–π –º—É–∫–∏.")
    elif float(message.text) < 135:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–Ω–∏–∂–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n–í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å –≥–µ–º–æ–≥–ª–æ–±–∏–Ω–∞ –Ω–∏–∂–µ –Ω–æ—Ä–º—ã. –°–æ–≤–µ—Ç—É–µ–º –æ–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ –≤—Ä–∞—á—É. –¢–∞–∫ –∂–µ –º–æ–∂–Ω–æ –ø–æ–¥–Ω—è—Ç—å –µ–≥–æ  –ø—Ä–∞–≤–∏–ª—å–Ω—ã–º –ø–∏—Ç–∞–Ω–∏–µ–º. –ù—É–∂–Ω–æ –µ—Å—Ç—å —Ç–∞–∫–∏–µ –ø—Ä–æ–¥—É–∫—Ç—ã:\n–ì—Ä–µ—á–∫–∞.\nK—É—Ä–∏—Ü–∞.\nK—É—Ä–∞–≥–∞.\n–°–≤–∏–Ω–∏–Ω–∞.")
    else:
        bot.send_message(message.chat.id, "–í—Å–µ –≤ –Ω–æ—Ä–º–µüòç")
        bot.send_message(message.chat.id, "–°–æ–≤–µ—Ç–∞ –Ω–µ –±—É–¥–µ—ÇüòÅ")
def tb_check(message):
    if float(message.text) > 400000:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–≤—ã—à–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n–í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å —Ç—Ä–æ–º–±–æ—Ü–∏—Ç–æ–≤ –≤—ã—à–µ –Ω–æ—Ä–º—ã. \n–í–∞–º –≤ –ø–µ—Ä–≤—É—é –æ—á–µ—Ä–µ–¥—å –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –æ–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ –≤—Ä–∞—á—É-–≥–µ–º–∞—Ç–æ–ª–æ–≥—É. –°–¥–∞—Ç—å –æ–±—â–∏–π –∞–Ω–∞–ª–∏–∑ –∫—Ä–æ–≤–∏ (–Ω–µ—Å–∫–æ–ª—å–∫–æ —Ä–∞–∑, —á—Ç–æ–±—ã –ø—Ä–æ–≤–µ—Ä–∏—Ç—å, —á—Ç–æ —ç—Ç–æ –Ω–µ –≤—Ä–µ–º–µ–Ω–Ω–æ–µ –ø–æ–≤—ã—à–µ–Ω–∏–µ), –∞–Ω–∞–ª–∏–∑ –Ω–∞ —É—Ä–æ–≤–µ–Ω—å –∂–µ–ª–µ–∑–∞, –º–∞—Ä–∫–µ—Ä—ã –≤–æ—Å–ø–∞–ª–µ–Ω–∏–π –∏ —Ä–∞–∫–∞, –ø—Ä–æ–π—Ç–∏ –æ—Å–º–æ—Ç—Ä —É —Å–ø–µ—Ü–∏–∞–ª–∏—Å—Ç–∞. \n–í—Ä–∞—á —É—Å—Ç–∞–Ω–æ–≤–∏—Ç –ø—Ä–∏—á–∏–Ω—É —Ç—Ä–æ–º–±–æ—Ü–∏—Ç–æ–∑–∞. –ï—Å–ª–∏ –æ–Ω–∏ –Ω–∏–∂–µ –Ω–æ—Ä–º—ã, —Ç–æ –Ω—É–∂–Ω–æ —Å—Ä–æ—á–Ω–æ –æ–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ —Å–ø–µ—Ü–∏–∞–ª–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–æ–º—É –≤—Ä–∞—á—É.")
    elif float(message.text) < 150000:
        bot.send_message(message.chat.id, "–í–∞—à —Ä–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–Ω–∏–∂–µ–Ω")
        bot.send_message(message.chat.id, "  –í–æ—Ç –≤–∞–º —Å–æ–≤–µ—Ç!\n–í–∞—à –ø–æ–∫–∞–∑–∞—Ç–µ–ª—å —Ç—Ä–æ–º–±–æ—Ü–∏—Ç–æ–≤ –Ω–∏–∂–µ –Ω–æ—Ä–º—ã. –í–∞–º –Ω—É–∂–Ω–æ —Å—Ä–æ—á–Ω–æ –æ–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ —Å–ø–µ—Ü–∏–∞–ª–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–æ–º—É –≤—Ä–∞—á—É.")
    else:
        bot.send_message(message.chat.id, "–í—Å–µ –≤ –Ω–æ—Ä–º–µüòç")
        bot.send_message(message.chat.id, "–°–æ–≤–µ—Ç–∞ –Ω–µ –±—É–¥–µ—ÇüòÅ")

@bot.message_handler(content_types=['text'])
def bot_message(message):
    if message.text == '–ù–∞–∑–∞–¥‚¨ÖÔ∏è':
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        item1 = types.KeyboardButton("–û–±—Ä–∞—Ç–∏—Ç—å—Å—è –∫ –≤—Ä–∞—á—Éüë©‚Äç‚öïÔ∏è")
        item2 = types.KeyboardButton("–ü—Ä–æ–≤–µ—Ä–∏—Ç—å—Å—è —É –±–æ—Ç–∞ü§ñ")
        markup.add(item1, item2)
        bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ –≤–∞—Ä–∏–∞–Ω—Ç –≤–æ–ø—Ä–æ—Å–∞:", reply_markup=markup)

if __name__ == "__main__":
    bot.infinity_polling()
